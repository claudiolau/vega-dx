{
  "name": "vega-dx",
  "version": "1.0.12",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "tsup": {
    "dts": true,
    "bundle": false,
    "treeshake": true,
    "target": "node18",
    "format": [
      "esm",
      "cjs"
    ],
    "entry": [
      "src/**/*.ts"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "plop": "plop",
    "prepare": "husky",
    "format": "biome check --apply-unsafe ./src",
    "build": "tsup",
    "changeset": "changeset",
    "version-package": "changeset version",
    "release": "pnpm run build && changeset publish",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "plop": "^4.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typedocs": "^0.6.7",
    "typescript": "^5.4.3"
  }
}
